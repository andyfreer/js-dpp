{
  "$schema": "http://json-schema.org/draft-07/schema",
  "properties": {
    "id": {
      "type": "string",
      "minLength": 42,
      "maxLength": 44,
      "pattern": "^[123456789ABCDEFGHJKLMNPQRSTUVWXYZabcdefghijkmnopqrstuvwxyz]+$"
    },
    "publicKeys": {
      "type": "array",
      "minItems": 1,
      "maxItems": 100
    },
    "balance": {
      "type": "integer",
      "minimum": 0
    },
    "$protocolVersion": {
      "type": "string",
      "minLength": 5,
      "pattern": "^(0|[1-9]\\d*)\\.(0|[1-9]\\d*)\\.(0|[1-9]\\d*)$"
    }
  },
  "required": [
    "id",
    "publicKeys",
    "balance",
    "$protocolVersion"
  ]
}
